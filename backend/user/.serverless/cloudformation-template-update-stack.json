{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "ServerlessDeploymentBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        }
      }
    },
    "ServerlessDeploymentBucketPolicy": {
      "Type": "AWS::S3::BucketPolicy",
      "Properties": {
        "Bucket": {
          "Ref": "ServerlessDeploymentBucket"
        },
        "PolicyDocument": {
          "Statement": [
            {
              "Action": "s3:*",
              "Effect": "Deny",
              "Principal": "*",
              "Resource": [
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      },
                      "/*"
                    ]
                  ]
                },
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      }
                    ]
                  ]
                }
              ],
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": false
                }
              }
            }
          ]
        }
      }
    },
    "UsersRouterLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/lambda/users-dev-usersRouter"
      }
    },
    "IamRoleLambdaExecution": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        },
        "Policies": [
          {
            "PolicyName": {
              "Fn::Join": [
                "-",
                [
                  "users",
                  "dev",
                  "lambda"
                ]
              ]
            },
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": [
                    "logs:CreateLogStream",
                    "logs:CreateLogGroup",
                    "logs:TagResource"
                  ],
                  "Resource": [
                    {
                      "Fn::Sub": "arn:${AWS::Partition}:logs:${AWS::Region}:${AWS::AccountId}:log-group:/aws/lambda/users-dev*:*"
                    }
                  ]
                },
                {
                  "Effect": "Allow",
                  "Action": [
                    "logs:PutLogEvents"
                  ],
                  "Resource": [
                    {
                      "Fn::Sub": "arn:${AWS::Partition}:logs:${AWS::Region}:${AWS::AccountId}:log-group:/aws/lambda/users-dev*:*:*"
                    }
                  ]
                },
                {
                  "Effect": "Allow",
                  "Action": [
                    "dynamodb:DescribeTable",
                    "dynamodb:GetItem",
                    "dynamodb:PutItem",
                    "dynamodb:UpdateItem",
                    "dynamodb:DeleteItem",
                    "dynamodb:BatchGetItem",
                    "dynamodb:Query",
                    "dynamodb:Scan"
                  ],
                  "Resource": [
                    "arn:aws:dynamodb:us-west-2:221025630675:table/UserProfiles",
                    "arn:aws:dynamodb:us-west-2:221025630675:table/ProjectInvitations",
                    "arn:aws:dynamodb:us-west-2:221025630675:table/ProjectInvitations/index/*"
                  ]
                }
              ]
            }
          }
        ],
        "Path": "/",
        "RoleName": {
          "Fn::Join": [
            "-",
            [
              "users",
              "dev",
              {
                "Ref": "AWS::Region"
              },
              "lambdaRole"
            ]
          ]
        }
      }
    },
    "UsersRouterLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "ServerlessDeploymentBucket"
          },
          "S3Key": "serverless/users/dev/1757537054170-2025-09-10T20:44:14.170Z/users.zip"
        },
        "Handler": "users/router.handler",
        "Runtime": "nodejs18.x",
        "FunctionName": "users-dev-usersRouter",
        "MemorySize": 256,
        "Timeout": 15,
        "Environment": {
          "Variables": {
            "ALLOWED_ORIGINS": "https://beta.mylg.studio,https://mylg.studio,http://localhost:3000",
            "CORS_WILDCARD_HOSTS": "mylg.studio",
            "CORS_ALLOW_CREDENTIALS": "false",
            "CORS_DEFAULT_ORIGIN": "https://beta.mylg.studio",
            "USER_PROFILES_TABLE": "UserProfiles",
            "INVITES_TABLE": "ProjectInvitations",
            "INVITES_BY_SENDER_INDEX": "senderId-index",
            "INVITES_BY_RECIPIENT_INDEX": "recipientId-index",
            "INBOX_TABLE": "Inbox",
            "MESSAGES_TABLE": "Messages",
            "PROJECT_MESSAGES_TABLE": "ProjectMessages",
            "NOTIFICATIONS_TABLE": "Notifications",
            "NOTIFICATIONS_BY_USER_INDEX": "userId-index",
            "PROJECTS_TABLE": "Projects",
            "TASKS_TABLE": "Tasks",
            "EVENTS_TABLE": "Events",
            "EVENTS_STARTAT_INDEX": "",
            "BUDGETS_TABLE": "Budgets",
            "BUDGET_ID_INDEX": "budgetId-index",
            "BUDGET_ITEM_ID_INDEX": "budgetItemId-index",
            "GALLERIES_TABLE": "Galleries",
            "GALLERIES_PROJECT_GSI": "projectId-index",
            "CONNECTIONS_TABLE": "Connections",
            "CONNECTIONS_USER_GSI": "userId-sessionId-index",
            "COGNITO_USER_POOL_ID": "placeholder",
            "COGNITO_USER_POOL_NAME": "mylg-dev-users-west2",
            "COGNITO_USER_CLIENT_ID": "placeholder",
            "SCANS_ALLOWED": "false"
          }
        },
        "Role": {
          "Fn::GetAtt": [
            "IamRoleLambdaExecution",
            "Arn"
          ]
        },
        "Layers": [
          "arn:aws:lambda:us-west-2:221025630675:layer:shared-utils:1"
        ]
      },
      "DependsOn": [
        "UsersRouterLogGroup"
      ]
    },
    "UsersRouterLambdaVersiongbGY5teQDmS3YDoAuxYLwIc3rvUKgJcEwE5uyaEgo4": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "UsersRouterLambdaFunction"
        },
        "CodeSha256": "dGFHw3VJ/wQUh33Sit2asKXDAt2LMV7MmbGONVuYFuY="
      }
    },
    "HttpApi": {
      "Type": "AWS::ApiGatewayV2::Api",
      "Properties": {
        "Name": "dev-users",
        "ProtocolType": "HTTP"
      }
    },
    "HttpApiStage": {
      "Type": "AWS::ApiGatewayV2::Stage",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "StageName": "$default",
        "AutoDeploy": true,
        "DefaultRouteSettings": {
          "DetailedMetricsEnabled": false
        }
      }
    },
    "UsersRouterLambdaPermissionHttpApi": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "FunctionName": {
          "Fn::GetAtt": [
            "UsersRouterLambdaFunction",
            "Arn"
          ]
        },
        "Action": "lambda:InvokeFunction",
        "Principal": "apigateway.amazonaws.com",
        "SourceArn": {
          "Fn::Join": [
            "",
            [
              "arn:",
              {
                "Ref": "AWS::Partition"
              },
              ":execute-api:",
              {
                "Ref": "AWS::Region"
              },
              ":",
              {
                "Ref": "AWS::AccountId"
              },
              ":",
              {
                "Ref": "HttpApi"
              },
              "/*"
            ]
          ]
        }
      }
    },
    "HttpApiIntegrationUsersRouter": {
      "Type": "AWS::ApiGatewayV2::Integration",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "IntegrationType": "AWS_PROXY",
        "IntegrationUri": {
          "Fn::GetAtt": [
            "UsersRouterLambdaFunction",
            "Arn"
          ]
        },
        "PayloadFormatVersion": "2.0",
        "TimeoutInMillis": 30000
      }
    },
    "HttpApiRouteAnyUserprofiles": {
      "Type": "AWS::ApiGatewayV2::Route",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "RouteKey": "ANY /userProfiles",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "HttpApiIntegrationUsersRouter"
              }
            ]
          ]
        }
      },
      "DependsOn": "HttpApiIntegrationUsersRouter"
    },
    "HttpApiRouteAnyUserprofilesProxyVar": {
      "Type": "AWS::ApiGatewayV2::Route",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "RouteKey": "ANY /userProfiles/{proxy+}",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "HttpApiIntegrationUsersRouter"
              }
            ]
          ]
        }
      },
      "DependsOn": "HttpApiIntegrationUsersRouter"
    },
    "HttpApiRouteAnyUserprofilespendingProxyVar": {
      "Type": "AWS::ApiGatewayV2::Route",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "RouteKey": "ANY /userProfilesPending/{proxy+}",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "HttpApiIntegrationUsersRouter"
              }
            ]
          ]
        }
      },
      "DependsOn": "HttpApiIntegrationUsersRouter"
    },
    "HttpApiRouteAnyInvitesProxyVar": {
      "Type": "AWS::ApiGatewayV2::Route",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "RouteKey": "ANY /invites/{proxy+}",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "HttpApiIntegrationUsersRouter"
              }
            ]
          ]
        }
      },
      "DependsOn": "HttpApiIntegrationUsersRouter"
    },
    "HttpApiRouteAnySendprojectinvitation": {
      "Type": "AWS::ApiGatewayV2::Route",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "RouteKey": "ANY /sendProjectInvitation",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "HttpApiIntegrationUsersRouter"
              }
            ]
          ]
        }
      },
      "DependsOn": "HttpApiIntegrationUsersRouter"
    },
    "HttpApiRouteAnyPostprojecttouserid": {
      "Type": "AWS::ApiGatewayV2::Route",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "RouteKey": "ANY /postProjectToUserId",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "HttpApiIntegrationUsersRouter"
              }
            ]
          ]
        }
      },
      "DependsOn": "HttpApiIntegrationUsersRouter"
    },
    "HttpApiRouteAnyUserHealth": {
      "Type": "AWS::ApiGatewayV2::Route",
      "Properties": {
        "ApiId": {
          "Ref": "HttpApi"
        },
        "RouteKey": "ANY /user/health",
        "Target": {
          "Fn::Join": [
            "/",
            [
              "integrations",
              {
                "Ref": "HttpApiIntegrationUsersRouter"
              }
            ]
          ]
        }
      },
      "DependsOn": "HttpApiIntegrationUsersRouter"
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      },
      "Export": {
        "Name": "sls-users-dev-ServerlessDeploymentBucketName"
      }
    },
    "UsersRouterLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "UsersRouterLambdaVersiongbGY5teQDmS3YDoAuxYLwIc3rvUKgJcEwE5uyaEgo4"
      },
      "Export": {
        "Name": "sls-users-dev-UsersRouterLambdaFunctionQualifiedArn"
      }
    },
    "HttpApiId": {
      "Description": "Id of the HTTP API",
      "Value": {
        "Ref": "HttpApi"
      },
      "Export": {
        "Name": "sls-users-dev-HttpApiId"
      }
    },
    "HttpApiUrl": {
      "Description": "URL of the HTTP API",
      "Value": {
        "Fn::Join": [
          "",
          [
            "https://",
            {
              "Ref": "HttpApi"
            },
            ".execute-api.",
            {
              "Ref": "AWS::Region"
            },
            ".",
            {
              "Ref": "AWS::URLSuffix"
            }
          ]
        ]
      },
      "Export": {
        "Name": "sls-users-dev-HttpApiUrl"
      }
    }
  }
}